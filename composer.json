{
	"name": "silverstripe/installer",
	"description": "The SilverStripe Framework Installer",
	"require": {
		"php": ">=5.3.2",
		"silverstripe/cms": "*",
		"silverstripe/framework": "*",
		"silverstripe-themes/simple": "*"
	},
	"require-dev": {
		"phpunit/PHPUnit": "~3.7@stable"
	},
	"config": {
		"process-timeout": 600
	},
	"minimum-stability": "dev",
	"extra": {
		"installer-paths": {
			"public_html/{$name}": [
				"silverstripe/cms",
				"silverstripe/framework"
			],
			"public_html/themes/{$name}": [
				"silverstripe-themes/simple"
			]
		}
	}
}
